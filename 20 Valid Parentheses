class Solution {
public:
    bool isValid(string s) {
        stack<char> t;
        for(int i = 0; i < s.size(); i++) {
            if(!t.empty() && t.top() == '(' && s[i] == ')' || !t.empty() && t.top() == '{' && s[i] == '}' || !t.empty() && t.top() == '[' && s[i] == ']')
            t.pop();
            else t.push(s[i]);
        }
        return t.empty();
    }
};


//in line 6 every scenario gotta consider !t.empty(), otherwise it shows up AddressSanitizer:DEADLYSIGNAL error.
