class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int res = 0;
        for(int i = 1; i < prices.size(); i++) {
            res += max(prices[i] - prices[i-1], 0);
        }
        return res;
    }
};


//method 2
class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int res = 0;
        for(int i = 0; i < prices.size() - 1; i++) {
            if(prices[i + 1] > prices[i]) res += prices[i + 1] - prices[i];
        }
        return res;
    }
};
