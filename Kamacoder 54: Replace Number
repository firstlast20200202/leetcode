#include <iostream>
using namespace std;

int main() {
    string s;
    while(cin >> s) {
        int indexOld = s.size()-1;
        int count = 0;
        for(int i = 0; i < s.size(); i++) {
            if(s[i] <= '9' && s[i] >= '0') {
                count++;
            }
        }

        s.resize(s.size()+count*5);
        int indexNew = s.size()-1;
        while(indexOld >= 0) {
            if(s[indexOld] >= '0' && s[indexOld] <= '9') {
                s[indexNew--] = 'r';
                s[indexNew--] = 'e';
                s[indexNew--] = 'b';
                s[indexNew--] = 'm';
                s[indexNew--] = 'u';
                s[indexNew--] = 'n';
            }
            else {
                s[indexNew--] = s[indexOld];
            }
            indexOld--;
        }
        cout << s << endl;
    }
}
