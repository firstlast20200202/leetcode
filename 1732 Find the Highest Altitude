class Solution {
public:
    int largestAltitude(vector<int>& gain) {
        int res = 0;
        vector<int> tmp(gain.size(), 0);
        for(int i = 1; i < gain.size(); i++) {
            tmp[i] = tmp[i-1] + gain[i-1];
            res = res > tmp[i] ? res : tmp[i];
        }
        return res;
    }
};
