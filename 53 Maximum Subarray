class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int res = INT_MIN, add = 0;
        for(int i = 0; i < nums.size(); i++) {
            add += nums[i];
            res = max(res, add);
            //res = add;
            if(add < 0) add = 0;
        }
        return res;
    }
};
