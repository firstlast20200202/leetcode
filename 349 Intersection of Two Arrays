//To transfer elements from a C++ std::vector to an std::set, the most efficient and common method is using the set's range constructor. 
//This automatically inserts all unique elements from the vector into the set upon creation, maintaining the set's sorted and unique properties. 

class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        unordered_set<int> tmp(nums1.begin(), nums1.end());
        unordered_set<int> res;
        for(int num : nums2) {
            if(tmp.find(num) != tmp.end()) {
                res.insert(num);
            }  
        }
        return vector<int>(res.begin(), res.end());
    }
};
